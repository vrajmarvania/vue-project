\n            <i class=\"fas fa-chevron-right\"></i>\r\n          </template>\r\n          <template #prev>\r\n           <i class=\"fas fa-chevron-left\"></i>\r\n          </template>\r\n        </navigation>\r\n      </template>\r\n\r\n  </Carousel>\r\n  \r\n      </div>\r\n    </div>\r\n    \r\n  </div>\r\n  </div>\r\n  \r\n\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent } from \"vue\";\r\nimport { Carousel, Navigation, Slide } from \"vue3-carousel\";\r\n\r\nimport \"vue3-carousel/dist/carousel.css\";\r\n// import card from \"./card.vue\";\r\n\r\nexport default defineComponent({\r\n  name: \"Breakpoints\",\r\n  components: {\r\n    Carousel,\r\n    Slide,\r\n    // card,\r\n    Navigation,\r\n  },\r\n  data: () => ({\r\n    imgs:[\"https://utioutll.sirv.com/Immortal%20Gaming/01_2.png\",\"https://utioutll.sirv.com/Immortal%20Gaming/01_3.png\",\"https://utioutll.sirv.com/Immortal%20Gaming/01_4.png\",\"https://utioutll.sirv.com/Immortal%20Gaming/01_5.png\",\"https://utioutll.sirv.com/Immortal%20Gaming/01_6.png\"],\r\n     settings: {\r\n      itemsToShow: 1,\r\n      snapAlign: \"center\",\r\n    },\r\n    // breakpoints are mobile first\r\n    // any settings not specified will fallback to the carousel settings\r\n     breakpoints: {\r\n      // 700px and up\r\n      1: {\r\n        itemsToShow: 1,\r\n        // snapAlign: \"center\",\r\n      },\r\n      700: {\r\n        itemsToShow: 1,\r\n        // snapAlign: \"center\",\r\n      },\r\n       900: {\r\n        itemsToShow: 2,\r\n        // snapAlign: \"center\",\r\n      },\r\n      // 1024 and up\r\n      1024: {\r\n        itemsToShow: 3,\r\n        snapAlign: \"start\",\r\n      },\r\n    }, \r\n  }),\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.carousel__prev--in-active,\r\n.carousel__next--in-active {\r\n  display: none;\r\n}\r\n\r\n.carousel {\r\n   \r\n    text-align: start;\r\n    \r\n}\r\n.carousel__prev {\r\n   \r\n    left: -25px;\r\n}\r\n.carousel__next  {\r\n   \r\n        right: -