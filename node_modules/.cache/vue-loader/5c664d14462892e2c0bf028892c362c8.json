collection\r\n      .getall_collection()\r\n      .then((response) => {\r\n        this.collection = response.data;\r\n        console.log(response.data);\r\n      })\r\n      .catch((e) => {\r\n        console.log(e);\r\n        this.errored = true;\r\n      })\r\n      .finally(() => {\r\n        this.loading = false;\r\n      });\r\n  },\r\n\r\n  watch: {\r\n    q: function () {\r\n      this.getdata(this.checkedNames, this.q);\r\n    },\r\n    checkedNames: function () {\r\n      this.getdata(this.checkedNames, this.q);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    onchnage() {\r\n      this.getdata(this.checkedNames, this.q);\r\n    },\r\n    getdata(cn, qu) {\r\n      search\r\n        .get_all(cn, qu)\r\n        .then((response) => {\r\n          this.datas = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          this.errored = true;\r\n        })\r\n        .finally(() => {\r\n          this.loading = false;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ni.fas.fa-chevron-down {\r\n  color: red;\r\n}\r\n</style>\r\n<style src=\"@vueform/slider/themes/default.css\"></style>\r\n"]}]}