\n          <input\r\n            type=\"text\"\r\n            v-bind:class=\"{\r\n              'form-control': true,\r\n            }\"\r\n            id=\"CategoryName\"\r\n            v-model=\"list.OfferName\"\r\n            required\r\n          />\r\n          <div id=\"CategoryType\" class=\"invalid-feedback\">\r\n            <!-- {{ errors.CategoryName }} -->\r\n          </div>\r\n        </div>\r\n        <b />\r\n        <div class=\"col-md-6\">\r\n          <label for=\"CategoryName\" class=\"form-label\">Offer Start Date</label>\r\n          <input\r\n            type=\"datetime-local\"\r\n            v-bind:class=\"{\r\n              'form-control': true,\r\n            }\"\r\n            id=\"CategoryName\"\r\n            v-model=\"list.Offer_Start_Date\"\r\n            required\r\n          />\r\n          <div id=\"CategoryType\" class=\"invalid-feedback\">\r\n            <!-- {{ errors.CategoryName }} -->\r\n          </div>\r\n        </div>\r\n        <b />\r\n        <div class=\"col-md-6\">\r\n          <label for=\"CategoryName\" class=\"form-label\">Offer End Date</label>\r\n          <input\r\n            type=\"datetime-local\"\r\n            v-bind:class=\"{\r\n              'form-control': true,\r\n            }\"\r\n            min=\"{{Offer_Start_Date}}\"\r\n            id=\"CategoryName\"\r\n            v-model=\"list.Offer_End_Date\"\r\n            required\r\n          />\r\n          <div id=\"CategoryType\" class=\"invalid-feedback\">\r\n            <!-- {{ errors.CategoryName }} -->\r\n          </div>\r\n        </div>\r\n        <b />\r\n        <div class=\"col-md-6\">\r\n          <label for=\"CategoryName\" class=\"form-label\">Offer PCT</label>\r\n          <input\r\n            type=\"number\"\r\n            v-bind:class=\"{\r\n              'form-control': true,\r\n            }\"\r\n            min=\"0\"\r\n            max=\"100\"\r\n            id=\"CategoryName\"\r\n            v-model=\"list.Offer_pct\"\r\n            required\r\n          />\r\n          <div id=\"CategoryType\" class=\"invalid-feedback\">\r\n            <!-- {{ errors.CategoryName }} -->\r\n          </div>\r\n        </div>\r\n        <b />\r\n        <div class=\"col-12\">\r\n          <button\r\n            class=\"btn fw-bolder\"\r\n            style=\"background-color: red; color: white\"\r\n            type=\"submit\"\r\n          >\r\n            Create Offer\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Offer from \"../../../Service/offer\";\r\n\r\nexport default {\r\n  name: \"Create-offer\",\r\n  data() {\r\n    return {\r\n      list: {},\r\n    };\r\n  },\r\n  methods: {\r\n    submit(e) {\r\n      console.log(this.list);\r\n\r\n      Offer.post_Offer(this.list)\r\n        .then((response) => {\r\n          if (response.data.errmsg == null) {\r\n            this.$swal.fire({\r\n              icon: \"success\",\r\n              title: \"Category success created\",\r\n              showConfirmButton: false,\r\n              timer: 1500,\r\n            });\r\n          this.$router.push(\"ManageOffer\");\r\n\r\n          } else {\r\n            this.$swal.fire({\r\n              icon: \"error\",\r\n              title: \"Oops...\",\r\n              text: response.data.errmsg,\r\n            });\r\n          }\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          this.errored = true;\r\n        })\r\n        .finally(() => {\r\n          this.loading = false;\r\n        });\r\n\r\n      e.preventDefault();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}